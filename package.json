{
  "name": "@scicave/math-latex-parser",
  "version": "1.2.1",
  "description": "A mathematical (La)TeX parser.",
  "main": "./lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scicave/math-latex-parser.git"
  },
  "bugs": {
    "url": "https://github.com/scicave/math-latex-parser/issues"
  },
  "homepage": "https://github.com/scicave/math-latex-parser#readme",
  "scripts": {
    "build": "node build.js",
    "buildtest": "npm run build && npm run test",
    "test": "jest ./tests",
    "lint": "eslint ./src/ --fix",
    "prod": "npm run lint && node generate-version-file.js && npm run build && npm run test",
    "debug": "node debug.js",
    "start": "npm run build:watch",
    "build:watch": "nodemon -w src -w tools -w build.js -i src/version.js -e js,peg -x 'npm run build'",
    "debug:watch": "nodemon -w src -w tools -w build.js -i src/version.js -e js,peg -x 'npm run build' -w debug.js -x 'npm run debug'",
    "test:watch": "jest ./tests --watch",
    "preversion": "npm run prod",
    "postversion": "npm run build"
  },
  "jest": {
    "verbose": true,
    "moduleNameMapper": {
      "parser": "<rootDir>/lib/index.js",
      "prepareInput": "<rootDir>/lib/prepareInput.js"
    }
  },
  "keywords": [
    "parser",
    "pegjs",
    "latex-parser",
    "math-parser",
    "math",
    "mathematics"
  ],
  "author": "scicave",
  "license": "MIT",
  "devDependencies": {
    "eslint": "^7.8.1",
    "jest": "^26.4.2",
    "nodemon": "^2.0.4",
    "pegjs": "^0.10.0",
    "rimraf": "^3.0.2"
  }
}
